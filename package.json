{
  "name": "tazel-home",
  "version": "1.0.0",
  "description": "Thomazella's homepage",
  "main": "./src/index.js",
  "scripts": {
    "start": "yarn parcel public/index.html --out-dir build --no-cache",
    "build": "yarn clean && yarn parcel build public/index.html --public-url ./ --out-dir build",
    "lint": "eslint src test",
    "next": "rimraf ../next-tazel-home/build && cp -RHi build ../next-tazel-home/",
    "clean": "yarn rimraf build && yarn rimraf .cache",
    "build:next": "yarn build && yarn next"
  },
  "repository": {
    "type": "git",
    "url": ".git"
  },
  "author": "Raphael Thomazella",
  "license": "SEE LICENSE IN LICENSE.TXT",
  "dependencies": {
    "fallback": "^1.0.1",
    "react": "^16.4.0",
    "react-router-dom": "^4.3.1",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^4.1.0",
    "gulp-rename": "^1.2.3",
    "imagemin-advpng": "^4.0.0",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-pngquant": "^5.1.0",
    "node-sass": "^4.9.0",
    "parcel": "^1.8.1",
    "png-to-jpeg": "^1.0.1",
    "prettier": "^1.13.4",
    "rimraf": "^2.6.2"
  }
}
